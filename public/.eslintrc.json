{
    "env": {
        "browser": true,
        "es2021": true,
        "jest/globals": true
    },
    "plugins": ["jest"],
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "import/extensions": "off",
        "no-await-in-loop": "off",
        "no-param-reassign": "off",
        "no-loop-func": "off",
        "no-restricted-syntax": "off",
        "import/no-unresolved": "off",
        "no-undef": "off",
        "import/no-extraneous-dependencies": "off",
        "no-redeclare": "off"
    }
}
